{
  "name": "Harboris Extension",
  "description": "The official Harboris extension",
  "version": "1.0.0",
  "manifest_version": 3,
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["contentScript.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
